// Test cases for the completed analyzeExpressionType function

// Test RangeExpression
fn testRange() {
    let r1 = 1..10;        // int array
    let r2 = "a".."z";     // string array
    let r3 = 0..;          // infinite range
}

// Test MapLiteral
fn testMapLiteral() {
    let m1 = {"a": 1, "b": 2};  // map<string, int>
    let m2 = {};                  // map<weak, weak>
}

// Test SetLiteral
fn testSetLiteral() {
    let s1 = {1, 2, 3};    // set<int>
    let s2 = {};           // set<weak>
}

// Test MemberExpression with method calls
fn testMemberExpression() {
    let arr = [1, 2, 3];
    let len = arr.length();
    arr.push(4);
}

fn main() {
    testRange();
    testMapLiteral();
    testSetLiteral();
    testMemberExpression();
}
