// Functions
do add(a, b) {
  return a + b
}

do factorial(n) {
  if n <= 1 {
    return 1
  }
  return n * factorial(n - 1)
}

print("5 + 3 =")
print(add(5, 3))

print("Factorial of 5:")
print(factorial(5))

