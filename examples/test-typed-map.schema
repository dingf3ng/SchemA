// Test explicit type parameters on Map constructor

// This should create a Map<int, int> and work correctly
do createIntMap() -> Map<int, int> {
  let m = Map<int, int>()
  m.set(1, 10)
  m.set(2, 20)
  return m
}

// This should create a Map<int, string> and work correctly
do createStringMap() -> Map<int, string> {
  let m = Map<int, string>()
  m.set(1, "hello")
  m.set(2, "world")
  return m
}

let intMap = createIntMap()
print("Int map:", intMap.get(1))

let strMap = createStringMap()
print("String map:", strMap.get(1))
